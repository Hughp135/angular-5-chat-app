{
  "name": "chat-angular-electron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:server:dev": "nodemon --watch back-end -e ts --exec \"npm run compile:server\"",
    "compile:server": "tsc -p back-end/tsconfig.json && node back-end/dist/index.js",
    "build": "ng build --prod",
    "test": "npm run test:server && npm run test:app",
    "test:app:dev": "ng test --browsers",
    "test:server:dev": "mocha --exit  -t 3000 -r ts-node/register \"back-end/src/**/*.spec.ts\"",
    "test:app": "ng test --code-coverage --watch=false",
    "test:server": "./node_modules/.bin/nyc --report-dir coverage mocha --exit -t 3000 -r ts-node/register \"back-end/src/**/*.spec.ts\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "ng lint && tslint -p back-end",
    "e2e": "ng e2e",
    "electron": "ng build && electron .",
    "electron-aot": "ng build --prod && electron ."
  },
  "main": "main.js",
  "private": true,
  "nyc": {
    "include": [
      "back-end/src/**/*.ts",
      "src/app/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "src/app/polyfills.ts",
      "src/app/test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "json"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@angular/cdk": "^5.2.1",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@ngrx/store": "^5.1.0",
    "@types/config": "0.0.33",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "config": "^1.29.4",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.1.1",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.6",
    "rxjs": "^5.5.6",
    "socket.io-client": "^2.0.4",
    "uninstall": "0.0.0",
    "winston": "^2.4.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/language-service": "^5.2.5",
    "@ngrx/store-devtools": "^5.1.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.10",
    "@types/express": "^4.11.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/joi": "^13.0.5",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.0.2",
    "@types/node": "~6.0.60",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "@types/socket.io-client": "^1.4.32",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "codelyzer": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "coveralls": "^3.0.0",
    "electron": "^1.8.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha": "^4.0.1",
    "mock-socket": "^7.1.0",
    "ng2-semantic-ui": "^0.9.6",
    "nyc": "^11.5.0",
    "protractor": "~5.1.2",
    "semantic-ui-less": "^2.2.14",
    "set-cookie-parser": "^2.0.0",
    "shared-interfaces": "file:shared-interfaces",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "uglifyjs-webpack-plugin": "^1.2.0"
  }
}

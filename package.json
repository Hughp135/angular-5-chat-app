    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "electron": "^1.8.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less": "^2.7.3",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^4.0.1",
    "mock-socket": "^7.1.0",
    "nyc": "^11.5.0",
    "protractor": "~5.1.2",
    "semantic-ui-less": "^2.2.14",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "~5.9.1"
  }
}

{
  "name": "chat-angular-electron",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:server:dev": "nodemon back-end/src/index.ts",
    "start:server:dev-old": "nodemon --watch back-end -e ts --exec \"npm run compile:server\"",
    "db:seed": "tsc -p back-end/tsconfig.json && node back-end/dist/tools/db-seeder.js",
    "start:server": "tsc -p back-end/tsconfig.json && node back-end/dist/index.js",
    "build": "ng build --prod -aot -vc -ec",
    "test": "npm run test:server && npm run test:app",
    "test:app:dev": "ng test --browsers --code-coverage",
    "test:server:dev": "mocha --exit  -t 10000 -r ts-node/register \"back-end/src/**/*.spec.ts\"",
    "test:app": "ng test --code-coverage --watch=false",
    "test:server": "./node_modules/.bin/nyc --report-dir coverage mocha --exit -t 10000 -r ts-node/register \"back-end/src/**/*.spec.ts\"",
    "artillery:test": "artillery run e2e/artillery-tests/text-chat-load-test.yaml",
    "compile:theme": "lessc --clean-css ./node_modules/semantic-ui-less/semantic.less src/assets/semantic.min.css",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "ng lint && tslint -p back-end",
    "e2e": "ng e2e",
    "electron": "node bin/build-electron-dev",
    "electron:prod": "node bin/build-electron-prod"
  },
  "main": "main.js",
  "private": true,
  "nyc": {
    "include": [
      "back-end/src/**/*.ts",
      "src/app/**/*.ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "src/app/polyfills.ts",
      "src/app/test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "json"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@angular/cdk": "^5.2.1",
    "@angular/cli": "1.7.3",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "^5.2.1",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@ngrx/store": "^5.1.0",
    "@ngrx/store-devtools": "^5.1.0",
    "@types/bcrypt": "^1.0.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.0.10",
    "@types/config": "0.0.33",
    "@types/express": "^4.11.0",
    "@types/joi": "^13.0.5",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.0.2",
    "@types/node": "~6.0.60",
    "@types/shortid": "0.0.29",
    "@types/simple-peer": "^6.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/winston": "^2.3.8",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "config": "^1.29.4",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "cropperjs": "^1.3.2",
    "express": "^4.16.2",
    "file-url": "^2.0.2",
    "hammerjs": "^2.0.8",
    "joi": "^13.1.2",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^8.2.1",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.14",
    "mongoose-delete": "^0.4.0",
    "ng2-right-click-menu": "file:vendor/ng2-right-click-menu-0.0.16.tgz",
    "ng2-semantic-ui": "^0.9.6",
    "ngx-clipboard": "^10.0.0",
    "rxjs": "^5.5.8",
    "set-cookie-parser": "^2.0.0",
    "shared-interfaces": "file:shared-interfaces",
    "shortid": "^2.2.8",
    "simple-peer": "^9.1.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.0.4",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "winston": "^2.4.0",
    "zone.js": "^0.8.20",
    "@types/socket.io": "^1.4.33"
  },
  "devDependencies": {
    "@angular/language-service": "5.2.9",
